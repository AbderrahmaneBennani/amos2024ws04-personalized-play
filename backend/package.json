    "@biomejs/biome": "1.9.4",
    "firebase-functions-test": "^3.1.0",
    "genkit-cli": "^0.9.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^4.9.5"
  },
